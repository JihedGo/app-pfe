{% extends 'app/index.html.twig' %}

{% block title %}Encadrements{% endblock %}


            {% block content %}
            <div id="layoutSidenav_content">
                <main>
                    <div class="container-fluid">
                        <h1 class="mt-4">Demandes</h1>
                        <ol class="breadcrumb mb-4">
                            <li class="breadcrumb-item"><a href="{{ path('app') }}">Dashboard</a></li>
                            <li class="breadcrumb-item active">Demandes</li>
                        </ol>

                        <div class="card mb-4">
                            <div class="card-header">
                                <i class="fas fa-table mr-1"></i>
                               Demande d'encadrement
                            </div>
                            <div class="card-body">
                                {{ form_start(form) }}
                                {{ form_widget(form) }}
                                <button type="submit" class="btn btn-sm btn-info mt-2">Envoyer</button>
                                {{ form_end(form) }}
                            </div>
                        </div>
                    </div>
                </main>
                {% include 'app/footer.html.twig' %}
            </div>
            {% endblock %}
